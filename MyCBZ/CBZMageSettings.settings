<?xml version='1.0' encoding='utf-8'?>
<SettingsFile xmlns="http://schemas.microsoft.com/VisualStudio/2004/01/settings" CurrentProfile="(Default)" GeneratedClassNamespace="MyCBZ" GeneratedClassName="CBZMageSettings">
  <Profiles />
  <Settings>
    <Setting Name="TempFolderPath" Type="System.String" Scope="User">
      <Value Profile="(Default)">%APPDATA%\CBZMage\temp\</Value>
    </Setting>
    <Setting Name="Version" Type="System.String" Scope="Application">
      <Value Profile="(Default)">0.5b</Value>
    </Setting>
    <Setting Name="RenamerPlaceholders" Type="System.Collections.Specialized.StringCollection" Scope="Application">
      <Value Profile="(Default)">&lt;?xml version="1.0" encoding="utf-16"?&gt;
&lt;ArrayOfString xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"&gt;
  &lt;string&gt;{name}&lt;/string&gt;
  &lt;string&gt;{title}&lt;/string&gt;
  &lt;string&gt;{page}&lt;/string&gt;
  &lt;string&gt;{pages}&lt;/string&gt;
  &lt;string&gt;{size}&lt;/string&gt;
  &lt;string&gt;{number}&lt;/string&gt;
  &lt;string&gt;{month}&lt;/string&gt;
  &lt;string&gt;{year}&lt;/string&gt;
  &lt;string&gt;{lang}&lt;/string&gt;
  &lt;string&gt;{series}&lt;/string&gt;
  &lt;string&gt;{type}&lt;/string&gt;
  &lt;string&gt;{if (&amp;lt;conditions&amp;gt;) then '&amp;lt;pattern&amp;gt;|&amp;lt;string&amp;gt;' else '&amp;lt;pattern&amp;gt;|&amp;lt;string&amp;gt;'}&lt;/string&gt;
&lt;/ArrayOfString&gt;</Value>
    </Setting>
    <Setting Name="StoryPageRenamePattern" Type="System.String" Scope="User">
      <Value Profile="(Default)">{title}_p{page}</Value>
    </Setting>
    <Setting Name="SpecialPageRenamePattern" Type="System.String" Scope="User">
      <Value Profile="(Default)">{type}_{index}</Value>
    </Setting>
  </Settings>
</SettingsFile>