    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <userSettings>
        <Win_CBZ.Win_CBZSettings>
            <setting name="TempFolderPath" serializeAs="String">
                <value>%APPDATA%\CBZMage\Temp\</value>
            </setting>
            <setting name="StoryPageRenamePattern" serializeAs="String">
                <value>p_{page}.{ext}</value>
            </setting>
            <setting name="SpecialPageRenamePattern" serializeAs="String">
                <value>{type}_{index}.{ext}</value>
            </setting>
            <setting name="PagePreviewEnabled" serializeAs="String">
                <value>False</value>
            </setting>
        </Win_CBZ.Win_CBZSettings>
        <Win_CBZ.CBZMageSettings>
